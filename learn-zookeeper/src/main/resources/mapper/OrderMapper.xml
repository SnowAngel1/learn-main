<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.learn.zookeeper.curator.mapper.OrderMapper">



    <insert id="insertOrder" parameterType="com.learn.zookeeper.curator.entity.Order">
        insert into user_order (pid, user_id)
        values (#{pId,jdbcType=VARCHAR}, #{userId,jdbcType=VARCHAR})
    </insert>
</mapper>